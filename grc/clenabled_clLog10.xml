<?xml version="1.0"?>
<block>
  <name>OpenCL Log10</name>
  <key>clenabled_clLog10</key>
  <category>[OpenCL-Enabled]</category>
  <import>import clenabled</import>
  <make>clenabled.clMathOp(2,$openCLPlatform,6)</make>
	<param>
		<name>OpenCL Platform</name>
		<key>openCLPlatform</key>
		<type>enum</type>
		<option>
			<name>GPU</name>
			<key>1</key>
		</option>
		<option>
			<name>Accelerator</name>
			<key>2</key>
		</option>
		<option>
			<name>CPU</name>
			<key>3</key>
		</option>
		<option>
			<name>Any</name>
			<key>4</key>
		</option>
	</param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
  </source>
	<doc>
This block is an OpenCL enabled version of the built-in Log10 block.
	</doc>
</block>
