<?xml version="1.0"?>
<block>
  <name>OpenCL Mag Phase To Complex</name>
  <key>clenabled_magphasetocomplex</key>
  <category>[OpenCL-Enabled]</category>
  <import>import clenabled</import>
  <make>clenabled.clMagPhaseToComplex($openCLPlatform,$setDebug)</make>
	<param>
		<name>OpenCL Platform</name>
		<key>openCLPlatform</key>
		<type>enum</type>
		<option>
			<name>GPU</name>
			<key>1</key>
		</option>
		<option>
			<name>Accelerator</name>
			<key>2</key>
		</option>
		<option>
			<name>CPU</name>
			<key>3</key>
		</option>
		<option>
			<name>Any</name>
			<key>4</key>
		</option>
	</param>
	<param>
		<name>Debug</name>
		<key>setDebug</key>
		<type>enum</type>
		<option>
			<name>Off</name>
			<key>0</key>
		</option>
		<option>
			<name>On</name>
			<key>1</key>
		</option>
	</param>
  <sink>
    <name>mag</name>
    <type>float</type>
  </sink>
  <sink>
    <name>phase</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
	<doc>
This block is an OpenCL enabled version of the built-in Mag Phase To Complex block.
	</doc>
</block>
